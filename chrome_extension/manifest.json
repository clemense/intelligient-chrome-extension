{
    "name": "Intelligient arXiv Abstracts",
    "description": "Shows how a paper contributes to the mission of solving intelligience.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": [
                "https://*.arxiv.org/abs/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    }
}